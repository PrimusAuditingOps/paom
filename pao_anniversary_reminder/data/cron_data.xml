<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="cron_anniversay_reminder_customers" model="ir.cron">
            <field name="name">Get customer to remind about audit anniversary</field>
            <field name="model_id" ref="pao_anniversary_reminder.model_pao_anniversary_reminder"/>
            <field name="state">code</field>
            <field name="code">model.get_customers_to_remind()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="numbercall">-1</field>
            <field name="doall">True</field>
            <field name="active">True</field>
        </record>
    </data>
</odoo>