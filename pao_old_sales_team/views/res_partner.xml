<?xml version="1.0" encoding="utf-8" ?>
<odoo>
     <record id="res_pao_old_sales_team_view_form" model="ir.ui.view">
        <field name="name">res.partner.pao.old.sales.team.view.form</field>
        <field name="model">res.partner</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
            
                <field name="team_id" position="after">
                    <field name="country_code" invisible="1">
                    <field name="pao_old_sales_team_id" invisible="country_code != 'MX'" options="{'no_quick_create': True, 'no_create_edit' : True}"/>
                </field>
            </field>
    </record>
</odoo>