<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="sale_report_view_tree_inherit" model="ir.ui.view">
            <field name="name">sale.report.view.tree.inherit</field>
            <field name="model">sale.report</field> 
            <field name="inherit_id" ref="sale.sale_report_view_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='order_reference']" position="after">
                    <field name="organization_id"/>
                    <field name="registry_number_id"/>
                    <field name="ship_date"/>
                    <field name="audit_date"/>
                    <field name="end_date"/>

                </xpath>
            </field>
        </record>
    </data>
</odoo>