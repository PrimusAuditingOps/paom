<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="hr_expense_product_product_expense_form_auditor_inherit" model="ir.ui.view">
        <field name="name">hr.expense.product.product.expense.form.auditor.inherit</field>
        <field name="model">product.product</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="hr_expense.product_product_expense_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='company_id']" position="after">
                <field name="category_for_auditors"/>
            </xpath>
        </field>
    </record>

</odoo>